---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

    <title>JSONDB - PHP Edition | Blog</title>
    <link href="http://php.jsondb.na2axl.tk/blog/atom.xml" rel="self"/>
    <link href="http://php.jsondb.na2axl.tk/blog"/>
    <updated>{{ site.time | date_to_xmlschema }}</updated>
    <id>http://php.jsondb.na2axl.tk/blog</id>
    <author>
        <name>Nana Axel</name>
        <email>ax.lnana@outlook.com</email>
    </author>

    {% for post in site.posts %}
    <entry>
        <title>{{ post.title }}</title>
        <link href="http://php.jsondb.na2axl.tk{{ post.url }}"/>
        <updated>{{ post.date | date_to_xmlschema }}</updated>
        <id>http://php.jsondb.na2axl.tk{{ post.id }}</id>
        <content type="html">{{ post.content | xml_escape }}</content>
    </entry>
    {% endfor %}

</feed>